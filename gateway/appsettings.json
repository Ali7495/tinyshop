{
  "ReverseProxy": {
    "Routs": {
      "identity": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/identity/{**catch-all}"
        }
      },
      "catalog": {
        "ClusterId": "catalog",
        "Match": {
          "Path": "/identity/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identity": {
        "Destination": {
          "d1": {
            "Address": "http://localhost:5011"
          }
        }
      },
      "catalog": {
        "Destination": {
          "d1": {
            "Address": "http://localhost:5022"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}